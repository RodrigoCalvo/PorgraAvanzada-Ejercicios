package items;

import mapa.Ubicacion;
import unidades.Elemento;

public class Capa extends UnidadDecorator {		
	 public Capa (Elemento elemento){
		 super(elemento);
		 this.elemento = elemento;
	 }
	 
	 @Override
	 public double getDaño() {
	  return this.elemento.getDaño() * 0.9;
	 }
	 @Override
	 public double getEnergia() {
	  return this.elemento.getEnergia() *2;
	 }
	 
	 @Override
	 public double getDefensa() {
		 return this.elemento.getDefensa();
	 }
		@Override
		public double getSalud() {
			// TODO Auto-generated method stub
			return 0;
		}

		@Override
		public Ubicacion getUbicacion() {
			// TODO Auto-generated method stub
			return null;
		}

		@Override
		public void atacar(Elemento e) {
			// TODO Auto-generated method stub
			
		}

		@Override
		public void meAtacan(double dañoRecibido) {
			// TODO Auto-generated method stub
			
		}

}
