package ejercicio;

import java.util.Arrays;


public class Ejercicio {

	public static void main(String[] args) {
		SopaDeLetra sopita = new SopaDeLetra("entrada.in");
		/*
		for (int i = 0; i < sopita.getSopaDeLetras().length; i++) {					
			System.out.println(Arrays.toString(sopita.getSopaDeLetras()[i]));			
		}
		*/
		long startTime, endTime, duration;

		startTime = System.nanoTime();
		sopita.prepararResultado();
		endTime = System.nanoTime();
		duration = (endTime - startTime);
		System.out.println("prepararResultado: 		" + duration + " nanosegundos");

		startTime = System.nanoTime();
		sopita.prepararResultadoOptimizado();
		endTime = System.nanoTime();
		duration = (endTime - startTime);
		System.out.println("prepararResultadoOptimizado: 	" + duration + " nanosegundos");

		startTime = System.nanoTime();
		sopita.prepararResultadoOptimizado2();
		endTime = System.nanoTime();
		duration = (endTime - startTime);
		System.out.println("prepararResultadoOptimizado2: 	" + duration + " nanosegundos");
	}

}
